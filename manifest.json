 {
    "name": "Language Learning Word Bank",
    "description" : "Stores words to learn for language learners",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup-home.html",
      "default_icon": "birch1.png"
    },
    "background":{
      "scripts": ["jquery-3.3.1.min.js", "popup-home.js"]
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens popup-home.html"
      }
    },
    "permissions": [
          "storage"
    ]
  }